
// Pipeline version
version = "0.4dev"

// Minimum version of nextflow required
nf_required_version = '0.27.0'

process {
  container = { "nf-core/tools:${workflow.revision ? workflow.revision : 'latest'}" }
  cpus = 1
  memory = 2.GB
  time = 14.h
}
params {
  outdir = './results'
  reads = "data/*.fastq"
  singleEnd = false
}

timeline {
  enabled = true
  file = "timeline.html"
}
report {
  enabled = true
  file = "report.html"
}
trace {
  enabled = true
  file = "trace.txt"
}

manifest {
  homePage = 'https://github.com/nf-core/tools'
  description = 'Minimal working example pipeline'
  mainScript = 'main.nf'
}
